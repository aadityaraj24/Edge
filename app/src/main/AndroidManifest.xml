<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.harsh.starringharsh.EDGE">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:theme="@style/Theme.AppCompat.Light"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true">

        <activity android:name="com.harsh.starringharsh.EDGE.MainMenu" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.harsh.starringharsh.EDGE.EventList" android:screenOrientation="portrait" />
        <activity android:name="com.harsh.starringharsh.EDGE.EventDetails" android:screenOrientation="portrait" />
        <activity android:name="com.harsh.starringharsh.EDGE.MainActivity" android:screenOrientation="portrait" />
        <activity android:name="com.harsh.starringharsh.EDGE.Sponsors" android:screenOrientation="portrait" />
        <activity android:name="com.harsh.starringharsh.EDGE.Team" android:screenOrientation="portrait" />
        <activity
            android:theme="@android:style/Theme.Dialog"
            android:name= "com.harsh.starringharsh.EDGE.TeamMember"
            android:screenOrientation="portrait" >
        </activity>
    </application>

</manifest>